{
  "manifest_version": 3,
  "name": "Infinite Craft Word Game",
  "version": "1.5",
  "description": "Find the selected words in Infinite Craft",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://neal.fun/infinite-craft/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "/icons/48.png",
    "128": "/icons/128.png"
  }
}

